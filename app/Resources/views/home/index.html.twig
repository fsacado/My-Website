{% extends 'base.html.twig' %}

{% block body %}
    <div data-section-name="identity" class="section" id="identity">
        {% include 'identity/index.html.twig' %}
    </div>
    <h1 class="center hidden-lg hidden-md">My projects</h1>
    {% for project in projects %}
        <div data-section-name="project{{ project.id }}" class="section" id="project{{ project.id }}">
            {% include 'project/project.html.twig' %}
        </div>
    {% endfor %}
{% endblock %}
